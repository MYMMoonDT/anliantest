<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<link rel="stylesheet" href="ui/css/form.css">
<div class="panel panel-default table-template">
	<div class="panel-heading">职业病危害项目评价报告书复核记录</div>
	<div class="panel-body">
		<form role="form">
			<div class="row">
				<div class="col-sm-6">
					<div class="form-group">
						<label>项目名称</label> <input type="text" class="form-control">
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6">
					<div class="form-group">
						<label>项目编号</label> <input type="text" class="form-control">
					</div>
				</div>
				<div class="col-sm-6">
					<div class="form-group">
						<label>编写人</label> <input type="text" class="form-control">
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6">
					<div class="form-group">
						<label>复核主持人</label> <input type="text" class="form-control">
					</div>
				</div>
				<div class="col-sm-6">
					<div class="form-group">
						<label>复核地点</label> <input type="text" class="form-control">
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6">
					<div class="form-group">
						<label>参加人员</label> <input type="text" class="form-control">
					</div>
				</div>
				<div class="col-sm-6">
					<div class="form-group">
						<label>时间</label> <input type="text" class="form-control">
					</div>
				</div>
			</div>
			<div class="row">
				<div class="container">
					<label>复核记录</label>
					<textarea rows="5" cols="90"></textarea>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6">
					<div class="form-group">
						<label>复核人签字</label> <input type="text" class="form-control">
					</div>
				</div>
				<div class="col-sm-6"></div>
			</div>
			<div class="row">

				<div class="container">
					<label>修改记录</label>
					<textarea rows="5" cols="90"></textarea>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6">
					<div class="form-group">
						<label>修改人签字</label> <input type="text" class="form-control" />
					</div>
				</div>
				<div class="col-sm-6"></div>
			</div>
		</form>
	</div>
</div>